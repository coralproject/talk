// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`renders correctly 1`] = `
<ForwardRef(forwardRef)
  data-testid="historyComment-comment-id"
>
  <Localized
    $title="Story Title"
    id="profile-historyComment-story"
  >
    <ForwardRef(forwardRef)
      variant="heading4"
    >
      Story: {$title}
    </ForwardRef(forwardRef)>
  </Localized>
  <Timestamp>
    2018-07-06T18:24:00.000Z
  </Timestamp>
  <ForwardRef(forwardRef)
    container="div"
    variant="bodyCopy"
  >
    <HTMLContent>
      Hello World
    </HTMLContent>
  </ForwardRef(forwardRef)>
  <ForwardRef(forwardRef)
    alignItems="center"
    direction="row"
    itemGutter={true}
  >
    <div
      className="HistoryComment-replies"
    >
      <ForwardRef(forwardRef)
        className="HistoryComment-icon"
      >
        reply
      </ForwardRef(forwardRef)>
      <Localized
        $replyCount={4}
        id="profile-historyComment-replies"
      >
        <span>
          Replies {$replyCount}
        </span>
      </Localized>
    </div>
    <ForwardRef(forwardRef)
      anchor={true}
      href="http://localhost/conversation"
      onClick={[Function]}
      target="_parent"
      variant="underlined"
    >
      <ForwardRef(forwardRef)>
        launch
      </ForwardRef(forwardRef)>
      <Localized
        id="profile-historyComment-viewConversation"
      >
        <span>
          View Conversation
        </span>
      </Localized>
    </ForwardRef(forwardRef)>
  </ForwardRef(forwardRef)>
  <MatchMediaWithContext
    lteWidth="xs"
  >
    <hr
      className="HistoryComment-divider"
    />
  </MatchMediaWithContext>
</ForwardRef(forwardRef)>
`;
